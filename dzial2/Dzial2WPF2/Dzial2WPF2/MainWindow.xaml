<Window x:Class="Dzial2WPF2.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Dzial2WPF2"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition/>

            <ColumnDefinition/>
        </Grid.ColumnDefinitions>
        <Grid.RowDefinitions>
            <RowDefinition/>
            <RowDefinition/>
        </Grid.RowDefinitions>
        <Button Content="Button" HorizontalAlignment="Center" Margin="170,54,0,0" VerticalAlignment="Center" Name="changeText" Click="changeText_Click"/>
        <CheckBox Content="Wlacz zmiane etykiety" x:Name="enableCheckBox" Grid.Column="1" HorizontalAlignment="Center" Margin="0,74,0,0" VerticalAlignment="Center"/>
        <TextBlock HorizontalAlignment="Center" Grid.Row="1"  TextWrapping="Wrap" Grid.ColumnSpan="2" VerticalAlignment="Top" Name="labelToChange" Width="220" />
        <TextBlock HorizontalAlignment="Center" Grid.Row="1"  TextWrapping="Wrap" Grid.ColumnSpan="2" VerticalAlignment="Center" Text="Nacisnij przycisk aby zmienic tekst" Name="dol" Width="220"/>
    </Grid>
</Window>
